<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="modle.di.tang.androidmodle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="modle.di.tang.androidmodle.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="modle.di.tang.android.Guest.GuestModelActivity"
            android:label="@string/geture">
            <intent-filter>
            <action android:name="modle.di.tang.action" />

            <category android:name="modle.di.tang.category" />
            <!--<category android:name="android.intent.category.DEFAULT" />-->
            </intent-filter>
        </activity>
        <activity android:name="modle.di.tang.android.Dialog.DialogActvivty"
            android:label="@string/dialog">
            <intent-filter>
                <action android:name="modle.di.tang.action" />

                <category android:name="modle.di.tang.category" />
            </intent-filter>
        </activity>
    </application>

</manifest>

